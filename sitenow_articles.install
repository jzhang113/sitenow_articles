<?php

//include helper file 
include_once(drupal_get_path('module','sitenow_articles') . '/sitenow_articles.helper.inc');

/**
* Hook Enable
*/

function sitenow_articles_enable() {
  //call function to create menu link
  sitenow_articles_create_menu();
}

/**
* Hook Disable
*/

function sitenow_articles_disable() {
  //call function to delete menu link
  sitenow_articles_delete_menu();
}